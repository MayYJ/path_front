<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.may.routeplansystem.dao.VehicleDao">    <resultMap id="vehicleMap" type="com.may.routeplansystem.entity.po.VehicleMessage">        <result property="vehicleId" column="vehicle_id" jdbcType="INTEGER"/>        <result property="questionId" column="question_id" jdbcType="INTEGER"/>        <result property="type" column="type" jdbcType="VARCHAR"/>        <result property="capacity" column="capacity" jdbcType="FLOAT"/>        <result property="oil" column="oil" jdbcType="FLOAT"/>        <result property="date" column="date" jdbcType="TIMESTAMP"/>        <result property="price" column="price" jdbcType="FLOAT"/>        <result property="ownerId" column="owner_id" jdbcType="INTEGER"/>    </resultMap>    <sql id="Base_Vehicle_List">        vehicle_id,question_id,type,capacity,oil,date,price,owner_id    </sql>    <sql id="insert_columns">        question_id,type,capacity,oil,date,price,owner_idve    </sql>    <insert id="insertVehicle" parameterType="com.may.routeplansystem.entity.po.VehicleMessage">        INSERT        INTO vehicle(question_id,type,capacity,oil,date,price,owner_id)        VALUES (#{questionId},#{type},#{capacity},#{oil},#{date},#{price},#{ownerId})    </insert>    <insert id="insertVehiclesBatch">        insert into vehicle(<include refid="insert_columns"/> )        values        <foreach collection="vehicleMessages" item="vehicle">            (#{vehicle.questionId},#{vehicle.type},#{vehicle.capacity},#{vehicle.oil}            ,#{vehicle.date},#{vehicle.price},#{vehicle.ownerId})        </foreach>    </insert>    <select id="searchVehicleByOwnId" parameterType="java.lang.Integer" resultMap="vehicleMap">        SELECT        <include refid="Base_Vehicle_List"/>        FROM vehicle        WHERE owner_id = #{ownerId}    </select>    <select id="searchVehicleByQuestionId" parameterType="int" resultMap="vehicleMap">        select <include refid="Base_Vehicle_List"/>        FROM vehicle        WHERE question_id = #{questionId}    </select>    <delete id="deleteVehicle">        DELETE        FROM vehicle        WHERE vehicle_id IN        <foreach item="deleteList" collection="list"                 open="(" separator="," close=")">            #{deleteList}        </foreach>    </delete>    <delete id="deleteVehicleByQuestionId" parameterType="int">        delete from vehicle        where question_id=#{questionId}    </delete>    <delete id="deleteVehicleByVehicleId" parameterType="int">        delete from vehicle        where vehicle_id=#{vehicleId}    </delete></mapper>